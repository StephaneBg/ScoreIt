<resources>

    <declare-styleable name="AdaptiveTheme">
        <attr name="isDialog" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SeekArc">
        <attr name="thumbOffset" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="progressWidth" format="dimension" />
        <attr name="arcWidth" format="dimension" />
        <attr name="progress" format="integer" />
        <attr name="arcColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="thumbColor" format="color" />
        <attr name="thumbRadius" format="dimension" />
        <attr name="clockwise" format="boolean" />
        <attr name="touchable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MaxWidthFrameLayout">
        <attr name="maxWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LineChart">
        <attr name="lineAxisColor" format="color" />
    </declare-styleable>
</resources>